{
  "name": "Nikso Solarized Light",
  "type": "light",
  "colors": {
    "focusBorder": "#d3af86",
    "selection.background": "#ccc4b0",
    "input.background": "#ddd6c1",
    "input.foreground": "#586e75",
    "inputOption.activeBorder": "#d3af86",
    "input.placeholderForeground": "#586e75aa",
    "dropdown.background": "#eee8d5",
    "dropdown.border": "#d3af86",
    "list.focusBackground": "#dfca8866",
    "list.activeSelectionBackground": "#dfca88",
    "list.activeSelectionForeground": "#6c6c6c",
    "list.inactiveSelectionBackground": "#d1cbb8",
    "list.hoverBackground": "#dfca8844",
    "list.highlightForeground": "#b58900",
    "pickerGroup.foreground": "#2aa19899",
    "pickerGroup.border": "#2aa19899",
    "button.background": "#ac9d57",
    "badge.background": "#b58900aa",
    "progressBar.background": "#b58900",
    "editor.background": "#fdf6e3",
    "editorWidget.background": "#eee8d5",
    "editor.selectionBackground": "#eee8d5",
    "editorHoverWidget.background": "#ccc4b0",
    "editor.lineHighlightBackground": "#eee8d5",
    "editorCursor.foreground": "#657b83",
    "editorWhitespace.foreground": "#586e7580",
    "peekViewTitle.background": "#eee8d5",
    "peekView.border": "#b58900",
    "peekViewResult.background": "#eee8d5",
    "peekViewEditor.background": "#fffbf2",
    "peekViewEditor.matchHighlightBackground": "#7744aa40",
    "tab.activeBackground": "#fdf6e3",
    "tab.inactiveBackground": "#d3cbb7",
    "tab.border": "#ddd6c1",
    "tab.inactiveForeground": "#586e75",
    "editorGroup.background": "#fffbf2",
    "editorGroupHeader.tabsBackground": "#d9d2c2",
    "editorGroup.border": "#ddd6c1",
    "editorGroup.dropBackground": "#ddd6c1aa",
    "panel.border": "#ddd6c1",
    "statusBar.foreground": "#586e75",
    "statusBar.background": "#eee8d5",
    "statusBar.noFolderBackground": "#eee8d5",
    "statusBarItem.prominentBackground": "#ddd6c1",
    "statusBarItem.prominentHoverBackground": "#ddd6c199",
    "activityBar.background": "#ddd6c1",
    "activityBar.foreground": "#584c27",
    "activityBar.dropBackground": "#eee8d5",
    "activityBarBadge.background": "#b58900",
    "sideBar.background": "#eee8d5",
    "sideBarTitle.foreground": "#586e75",
    "titleBar.activeBackground": "#eee8d5",
    "notification.background": "#999178",
    "debugExceptionWidget.border": "#ab395b",
    "debugExceptionWidget.background": "#ddd6c1",
    "debugToolBar.background": "#ddd6c1",
    "statusBar.debuggingBackground": "#eee8d5",
    "extensionButton.prominentBackground": "#b58900",
    "extensionButton.prominentHoverBackground": "#584c27aa",
    "terminal.ansiBlack": "#073642",
    "terminal.ansiRed": "#dc322f",
    "terminal.ansiGreen": "#859900",
    "terminal.ansiYellow": "#b58900",
    "terminal.ansiBlue": "#268bd2",
    "terminal.ansiMagenta": "#d33682",
    "terminal.ansiCyan": "#2aa198",
    "terminal.ansiWhite": "#eee8d5",
    "terminal.ansiBrightBlack": "#586e75",
    "terminal.ansiBrightRed": "#cb4b16",
    "terminal.ansiBrightGreen": "#586e75",
    "terminal.ansiBrightYellow": "#657b83",
    "terminal.ansiBrightBlue": "#839496",
    "terminal.ansiBrightMagenta": "#6c71c4",
    "terminal.ansiBrightCyan": "#93a1a1",
    "terminal.ansiBrightWhite": "#eee8d5"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#657b83ff",
        "background": "#fdf6e3ff"
      }
    },
    {
      "name": "HTML/XML: Content",
      "scope": "text.html.basic, text.xml",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "HTML/XML: Tag",
      "scope":
        "punctuation.definition.tag.html, punctuation.definition.tag.begin.html, punctuation.definition.tag.end.html, punctuation.definition.tag.xml, meta.tag.other.html, meta.tag.any.html",
      "settings": {
        "foreground": "#657b83"
      }
    },
    {
      "name": "HTML/XML: Tag name",
      "scope":
        "punctuation.definition.tag.name.html, punctuation.definition.tag.name.xml, entity.name.tag",
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "name": "Comment",
      "scope": "comment, comment.block.documentation.javadoc",
      "settings": {
        "foreground": "#93a1a1"
      }
    },
    {
      "name": "HTML/XML: Attribute",
      "scope":
        "entity.other.attribute-name.html, entity.other.attribute-name.xml, entity.other.attribute-name.localname.xml",
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "name": "HTML/XML: DOCTYPE",
      "scope": "meta.tag.sgml.doctype.html",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "name": "CSS: Tag",
      "scope": "css.entity.name.tag, scss.entity.name.tag",
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "name": "CSS/SCSS: Property name",
      "scope":
        "support.type.property-name.css, support.type.property-name.scss",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "CSS/SCSS: Class",
      "scope":
        "entity.other.attribute-name.class, entity.other.attribute-name.parent-selector-suffix.scss",
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "name": "CSS/SCSS: Unit",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#657b83",
        "fontStyle": "normal"
      }
    },
    {
      "name": "SCSS: Entity reference",
      "scope": "entity.name.tag.reference.scss",
      "settings": {
        "foreground": "#073642"
      }
    },
    {
      "name": "CSS/SCSS: Function call",
      "scope":
        "support.function.css, support.function.misc.scss, entity.name.function.scss",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "SCSS: Variable",
      "scope": "variable.scss",
      "settings": {
        "foreground": "#6c71c4"
      }
    },
    {
      "name": "CSS/SCSS: Id",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#268bd2",
        "fontStyle": " bold"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#6c71c4"
      }
    },
    {
      "name": "Keyword",
      "scope":
        "keyword, keyword.operator.static, keyword.operator.new, keyword.operator.this, keyword.operator.expression, keyword.operator.delete, storage.type, storage.modifier, storage.arrow, variable.language.this, constant.language.boolean, constant.language.null, constant.language.json, constant.language.java, variable.language.java, constant.language.php",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D33811"
      }
    },
    {
      "name": "Braces, brackets, operators",
      "scope":
        "punctuation.section, keyword.operator.assignment, keyword.operator.arithmetic, keyword.operator.ternary, punctuation.terminator, punctuation.definition.block, punctuation.accessor, keyword.operator.dereference.java, punctuation.definition.parameters, meta.brace.round.js, meta.brace.square.js, meta.method.identifier.java, keyword.operator.increment-decrement.java, keyword.control.ternary.java, keyword.operator.comparison, keyword.operator.relational, keyword.operator.logical, keyword.operator.css, keyword.operator.class.php, keyword.operator.key.php",
      "settings": {
        "foreground": "#073642"
      }
    },
    {
      "name": "Class",
      "scope":
        "entity.name.type.class, storage.type.java, support.class.php, support.other.namespace.use-as.php, entity.other.inherited-class.php, meta.other.inherited-class.php, entity.other.inherited-class.ts",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "String",
      "scope": "string.quoted, string.regexp",
      "settings": {
        "foreground": "#2aa198"
      }
    },
    {
      "name": "CSS/SCSS: Pseudo element/class",
      "scope":
        "entity.other.attribute-name.pseudo-element, entity.other.attribute-name.pseudo-class",
      "settings": {
        "foreground": "#b58900",
        "fontStyle": " italic"
      }
    },
    {
      "name": "JS: Function name",
      "scope": "support.function, entity.name.function",
      "settings": {
        "fontStyle": "normal",
        "foreground": "#268bd2"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#657b83"
      }
    },
    {
      "name": "Variable, property",
      "scope": "variable.property, variable.other.readwrite.js",
      "settings": {
        "foreground": "#657b83"
      }
    },
    {
      "name": "Java: Constant",
      "scope": "constant.other.java, constant.other.definition.java",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "JS Doc: Tag",
      "scope": "storage.type.class.jsdoc",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#93a1a1"
      }
    },
    {
      "name": "JS Doc: Param type",
      "scope": "entity.name.type.instance.jsdoc",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Instance field",
      "scope":
        "variable.property, variable.constant, variable.other.property, variable.other.constant, block.support.variable.property, block.support.function.variable.property, variable.other.object.property",
      "settings": {
        "foreground": "#D8A90F"
      }
    },
    {
      "name": "Java: Annotation",
      "scope": "storage.type.annotation.java",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#b58900"
      }
    },
    {
      "name": "JavaDoc: Tag",
      "scope":
        "keyword.other.documentation.return.javadoc, keyword.other.documentation.param.javadoc, keyword.other.documentation.see.javadoc",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#93a1a1"
      }
    },
    {
      "name": "Java: Imports",
      "scope": "storage.modifier.import.java",
      "settings": {
        "foreground": "#859900",
        "fontStyle": "normal"
      }
    },
    {
      "name": "JSON: Property name",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "name": "PHP: tag",
      "scope": "punctuation.section.embedded.begin.metatag.php",
      "settings": {
        "foreground": "#dc322f",
        "fontStyle": "bold"
      }
    },
    {
      "name": "PHP: support function",
      "scope": "support.function.array.php",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "PHP: variable",
      "scope": "variable.other.php, punctuation.definition.variable.php",
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "name": "PHP: Constant",
      "scope": "constant.other.php, constant.other.class.php",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "PHP: Builtin class",
      "scope": "support.class.builtin.php",
      "settings": {
        "foreground": "#859900",
        "fontStyle": "bold"
      }
    },
    {
      "name": "PHP Doc: Keyword",
      "scope": "keyword.other.phpdoc.php",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#93a1a1"
      }
    },
    {
      "name": "TypeScript: type",
      "scope": "entity.name.type.ts, support.type.primitive.ts",
      "settings": {
        "foreground": "#af8700"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#316bcd"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#cd3131"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#800080"
      }
    }
  ]
}
